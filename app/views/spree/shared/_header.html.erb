<div id="spree-header">
  <header id="header" data-hook>
    <div id="navigation" class="container-fluid">
      <div class="row">
        <div id="fill" class='col-xs-3'></div>
        <figure id="logo" class='col-xs-6' data-hook>
          <div class="row">
            <div id="logocenter" class="col-xs-12">

              <div class="navLogo">
                <%= logo %>
              </div>
            </div>
          </div>

        </figure>
        <div id="navButtons" class='col-xs-3'>
          <div class="row">
            <div id="user" class='col-xs-4 visible-sm visible-md visible-lg'>
              <a href="/account" class="navUser">
                <!-- <img src="assets/user.png" class="user-icon"> -->
              </a>
            </div>
            <div id="shopcart" class='col-xs-4 visible-sm visible-md visible-lg'>
              <a href="/cart" class="navCart">
                <!-- <img src="assets/user.png" class="user-icon"> -->
              </a>
              <!-- <img src="assets/shop.png" class="cart-icon"> -->
            </div>

            <div id="hamburger" class='col-xs-4'>
              <div id="nav-icon1" class=" open-burger">
                <span></span>
                <span></span>
              </div>
            </div>

          </div>
        </div>
      </div>

      <div id="menuBar" class="menuBar">

        <div class="menuText row">
          <a href="/" class="col-sm-3 homelink">home

          </a>
          <a href="/shop" class="col-sm-3 shoplink">shop

          </a>
          <a href="/about" class="col-sm-3 aboutlink">our story

          </a>
          <a href="/account" class="col-sm-3 loginlink">login

          </a>

        </div>
      </div>

    </div>
  </header>
</div>
