<div id="spree-header">
  <header id="header" data-hook>
    <div class="container-fluid">
      <div class="row">
        <div id="fill" class='col-xs-3'></div>
        <figure id="logo" class='col-xs-6' data-hook>
          <div class="row">
<div id="logocenter" class="col-xs-12">

            <div class="navLogo">
              <%= logo %>
            </div>
          </div>
          </div>

        </figure>
        <div id="navButtons" class='col-xs-3'>
          <div class="row">
            <div id="user" class='col-xs-4'>
              <a href="/account" class="navUser">
                <!-- <img src="assets/user.png" class="user-icon"> -->
              </a>
            </div>
            <div id="cart" class='col-xs-4'>
              <a href="/cart" class="navCart">
                <!-- <img src="assets/user.png" class="user-icon"> -->
              </a>
              <!-- <img src="assets/shop.png" class="cart-icon"> -->
            </div>
            <div id="hamburger" class='col-xs-4'>
              <div id="nav-icon1" class=" open-burger">
                <span></span>
                <span></span>
              </div>
            </div>
          </div>
        </div>

      </div>
    </div>
  </header>
  <div class="container">
    <%= render partial: 'spree/shared/main_nav_bar' if store_menu? %>
  </div>
</div>
